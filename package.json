{
  "name": "ilp-plugin-bigchaindb",
  "version": "0.0.7",
  "description": "ILP Ledger plugin for BigchainDB",
  "homepage": "https://www.bigchain.io/",
  "bugs": "https://github.com/bigchain/js-ilp-plugin-bigchaindb/issues",
  "license": "Apache-2.0",
  "author": "BigchainDB Team <info@bigchaindb.com>",
  "files": [
    "*.md",
    "bundle",
    "cjs",
    "es6"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:bigchaindb/js-ilp-plugin-bigchaindb.git"
  },
  "keywords": [
    "bigchaindb",
    "blockchain",
    "interledger",
    "plugin",
    "connector",
    "ilp"
  ],
  "main": "cjs/lib/bigchaindb_ledger_plugin.js",
  "jsnext:main": "es6/lib/bigchaindb_ledger_plugin.js",
  "directories": {
    "lib": "cjs/"
  },
  "scripts": {
    "lint": "eslint ./",
    "build": "npm run build:bundle && npm run build:cjs && npm run build:es6",
    "build:bundle": "rimraf ./bundle && cross-env NODE_ENV=production BABEL_ENV=bundle webpack -p",
    "build:cjs": "rimraf ./cjs && cross-env BABEL_ENV=cjs babel ./src -d cjs",
    "build:es6": "rimraf ./es6 && cross-env babel ./src -d es6",
    "clean": "rimraf ./build ./bundle ./cjs ./es6",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "co": "^4.6.0",
    "co-request": "^1.0.0",
    "core-js": "^2.4.0",
    "es6-promise": "^3.2.1",
    "eventemitter2": "^1.0.3",
    "js-utility-belt": "^1.3.1",
    "reconnect-core": "^1.2.0",
    "simple-websocket": "^4.1.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-es2015-no-commonjs": "0.0.2",
    "babel-runtime": "^6.9.2",
    "cross-env": "^1.0.8",
    "eslint": "^2.10.2",
    "eslint-config-ascribe": "^2.0.0",
    "eslint-plugin-import": "^1.8.0",
    "rimraf": "^2.5.2",
    "webpack": "=2.1.0-beta.17",
    "webpack-combine-loaders": "^2.0.0"
  }
}
